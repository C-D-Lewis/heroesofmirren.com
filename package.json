  },
  "author": "Chris Lewis",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "http-server": "^14.1.1"
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb-base",
    "env": {
      "browser": true
    },
    "rules": {
      "no-multi-spaces": 0,
      "global-require": 0,
      "import/no-dynamic-require": 0,
      "no-await-in-loop": 0,
      "no-console": 0,
      "no-alert": 0,
      "no-param-reassign": [
        "error",
        { "props": false }
      ]
    },
    "globals": {
      "fabricate": "readonly",
      "Theme": "readonly",
      "Utils": "readonly",
      "Assets": "readonly",
      "FavoritesService": "readonly",
      "AudioService": "readonly"
    }
  }
